            }
         }
      }
   }
})

db.createCollection("Student", {
   validator: {
      $jsonSchema: {
         bsonType: "object",
         required: ["full_name", "date_of_birth", "group_id", "budget"],
         properties: {
            id: {
               bsonType: "int",
               description: "Уникальный идентификатор студента"
            },
            full_name: {
               bsonType: "string",
               description: "ФИО студента"
            },
            date_of_birth: {
               bsonType: "date",
               description: "Дата рождения"
            },
            address: {
               bsonType: "string",
               description: "Адрес проживания"
            },
            email: {
               bsonType: "string",
               description: "Адрес электронной почты"
            },
            group_id: {
               bsonType: "int",
               description: "Идентификатор группы"
            },
            budget: {
               bsonType: "bool",
               description: "Признак бюджетного/внебюджетного обучения"
            }
         }
      }
   }
})

