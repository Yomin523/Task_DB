db.createCollection("Directions_of_study", {
   validator: {
      $jsonSchema: {
         bsonType: "object",
         required: ["direction_name"],
         properties: {
            id: {
               bsonType: "int",
               description: "Уникальный идентификатор направления"
            },
            direction_name: {
               bsonType: "string",
               description: "Название направления"
            }
         }
      }
   }
})

db.createCollection("Groups", {
   validator: {
      $jsonSchema: {
         bsonType: "object",
